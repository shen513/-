#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[])
{
   int a=20,b=15;
    
    { 
//scanf("%d %d",&a,&b); 
printf("%d+%d=%d\n",a,b,a+b);
printf("%d*%d=%d\n",a,b,a*b); 
printf("%d-%d=%d\n",a,b,a-b); 
printf("%d/%d=%d\n",a,b,a/b); 
        
        }
  
  
  
  system("PAUSE");	
  return (0);
}
